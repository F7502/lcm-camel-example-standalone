<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cxf="http://camel.apache.org/schema/blueprint/cxf"
	xsi:schemaLocation="
      http://www.osgi.org/xmlns/blueprint/v1.0.0
      http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<camelContext id="examplecontextforlcmlabs"
		xmlns="http://camel.apache.org/schema/blueprint">
		<!-- <package>de.ustutt.iaas.lcm.labs.example</package> -->
		<routeBuilder ref="routeBuilder" />
		<propertyPlaceholder id="properties"
			location="src/main/resources/my.properties" />
	</camelContext>

	<bean id="routeBuilder"
		class="de.ustutt.iaas.lcm.labs.standalone.MyRouteBuilder" />

	<bean id="foo"
		class="de.ustutt.iaas.lcm.labs.standalone.MyBean" />

</blueprint>